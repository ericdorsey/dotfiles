set number
syntax on
set t_Co=256
"set t_Co=16
set background=dark
set shiftwidth=4
set tabstop=4
set expandtab
set termguicolors "vivid colors

let g:gruvbox_italic=0
let g:gruvbox_contrast_dark = 'hard'
"let g:gruvbox_colors = { 'bg0': ['#000000', 0] }
"let g:gruvbox_termcolors=16
colorscheme gruvbox
